generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum Gender {
  MALE
  FEMALE
  OTHER
}

model User{
  id String  @id @default(uuid())
  name String 
  email String @unique
  gender Gender? 
  oauth_id String? @unique
  password String? 
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt()

  @@map("users")
}

